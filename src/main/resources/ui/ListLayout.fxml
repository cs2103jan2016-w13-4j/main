<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>

<AnchorPane maxHeight="670.0" maxWidth="650.0" prefHeight="670.0" prefWidth="650.0" style="-fx-background-color: #f7c873;" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="jfdi.ui.MainController">
   <children>
      <StackPane maxHeight="670.0" maxWidth="650.0" prefHeight="670.0" prefWidth="650.0" VBox.vgrow="ALWAYS">
          <children>
	          <HBox maxHeight="670.0" maxWidth="650.0" prefHeight="670.0" prefWidth="650.0">
	              <children>
				      <VBox fx:id="backDrop" prefHeight="670.0" prefWidth="200.0" style="-fx-background-color: f7c873;">
				         <children>
				            <Label fx:id="dayDisplayer" alignment="CENTER" contentDisplay="CENTER" prefHeight="68.0" prefWidth="200.0" styleClass="dayDate" stylesheets="@sideBar.css" textAlignment="CENTER" wrapText="true" />
				            <ListView fx:id="statsDisplayer" prefHeight="70.0" prefWidth="200.0" style="-fx-background-color: #434343;" stylesheets="@sideBar.css">
				               <VBox.margin>
				                  <Insets top="4.0" />
				               </VBox.margin>
				            </ListView>
				            <Label fx:id="overdueLabel" alignment="CENTER" layoutX="0.0" layoutY="139.0" prefHeight="30.0" prefWidth="200.0" styleClass="overdue" stylesheets="@sideBar.css" text="Overdue">
				               <VBox.margin>
				                  <Insets top="4.0" />
				               </VBox.margin>
				            </Label>
				            <ListView fx:id="overdueList" prefHeight="230.0" prefWidth="200.0" style="-fx-background-color: #434343;" styleClass="overduelist" stylesheets="@sideBar.css" />
				            <Label fx:id="upcomingLabel" alignment="CENTER" layoutX="0.0" layoutY="405.0" prefHeight="30.0" prefWidth="200.0" styleClass="upcoming" stylesheets="@sideBar.css" text="Upcoming">
				               <VBox.margin>
				                  <Insets top="4.0" />
				               </VBox.margin>
				            </Label>
				            <ListView fx:id="upcomingList" prefHeight="230.0" prefWidth="195.0" styleClass="upcomingList" stylesheets="@sideBar.css" />
				         </children>
				      </VBox>
				      <VBox layoutX="200.0" prefHeight="670.0" prefWidth="450.0">
				         <children>
				            <StackPane prefHeight="65.0" prefWidth="450.0" style="-fx-background-color: #faebcd;">
				               <children>
				                  <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#434343" height="40.0" stroke="#434343" strokeLineCap="ROUND" strokeLineJoin="ROUND" strokeWidth="10.0" translateZ="5.0" width="400.0">
				                     <StackPane.margin>
				                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
				                     </StackPane.margin>
				                  </Rectangle>
				                  <Label fx:id="listStatus" prefHeight="40.0" prefWidth="408.0" styleClass="listStatus" text="Display List" stylesheets="@jfdiLook.css"/>
				               </children>
				            </StackPane>
				            <ListView fx:id="listMain" prefHeight="500.0" prefWidth="450.0" style="-fx-background-color: faebcd;" stylesheets="@jfdiLook.css">
				               <VBox.margin>
				                  <Insets />
				               </VBox.margin>
				            </ListView>
				            <TextArea fx:id="fbArea" styleClass="fbArea" blendMode="MULTIPLY" prefHeight="50.0" prefWidth="430.0" stylesheets="@jfdiLook.css">
				               <VBox.margin>
				                  <Insets bottom="5.0" left="10.0" right="10.0" top="7.0" />
				               </VBox.margin>
				            </TextArea>
				            <TextField fx:id="cmdArea" styleClass="cmdArea" blendMode="SRC_ATOP" maxHeight="35.0" maxWidth="430.0" prefHeight="25.0" prefWidth="430.0" promptText="(Hey Jim! Please let me know what I can do for you!)" stylesheets="@jfdiLook.css">
				               <VBox.margin>
				                  <Insets left="10.0" right="10.0" top="5.0" />
				               </VBox.margin>
				            </TextField>
				         </children>
				      </VBox>
			       </children>
			   </HBox>
			   <VBox fx:id="helpOverlay" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="550.0" prefWidth="500.0" StackPane.alignment="CENTER">
	               <children>
	                  <Label fx:id="helpIcon" focusTraversable="false" text="Help Icon" />
	                  <Label fx:id="helpTitle" focusTraversable="false" text="Help title" />
	                  <ListView fx:id="helpContent" focusTraversable="false" maxWidth="-Infinity" prefWidth="400.0" />
	               </children>
               </VBox>
		   </children>
      </StackPane>
   </children>
</AnchorPane>
